!!! 5
html(lang="en")
	include /head
	body
		include /titlebar
		.container
			include /navbar
			#content
				div
					=givens
					br
					i = 0
					each row in userInput	
						i = i + 1	
						j = 0
						each col in row	
							j = j + 1
							=col
							if (j % 9 != 0)
								&nbsp;&nbsp;&nbsp;
							if (j % 3 == 0)
								|&nbsp;
								=tempData
						br
						if (i % 3 == 0)
							br
	include /footer
						
								
